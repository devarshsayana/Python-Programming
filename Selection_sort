def selection_sort(arr):
    for i in range(len(arr)):
        min_index=i
        for j in range(i+1,len(arr)):
            if(arr[j]<arr[min_index]):
                min_index=j
        temp=arr[min_index]
        arr[min_index]=arr[i]
        arr[i]=temp
    return arr

n=int(input("Enter no of elements:"))
arr=[]
for i in range(n):
    arr.append((int(input())))
print(selection_sort(arr))
