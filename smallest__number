def smallest():
    smallest =arr[0]
    for i in range(1,len(arr)):
        if(smallest>=arr[i]):
            smallest=arr[i]
    return smallest
n=int(input("Enter number of elementts:"))
arr=[]
for i  in range(n):
    x=int(input("Enter new number:"))
    arr.append(x)
print(arr)
print(smallest())
