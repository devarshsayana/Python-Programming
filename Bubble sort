def bubblesort():
    l=len(arr)
    for i in range(1,l):
        for j in range(0,l-1):
            if arr[j]>arr[j+1]:
                temp=arr[j]
                arr[j]=arr[j+1]
                arr[j+1]=temp
    return arr
arr=[]
n=int(input("How manay elemnts in the array:"))
for i in range(n):
    arr.append(int(input("Enter Element")))
print(bubblesort())
        
